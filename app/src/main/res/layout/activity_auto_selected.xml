<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Cliente.Home.AutoSelected">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/backarrow"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/baseline_arrow_back_18"
            android:onClick="back"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"/>
        <ImageView
            android:id="@+id/img"
            android:layout_width="256dp"
            android:layout_height="160dp"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/AModelo"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Modelo"
            android:id="@+id/AModelo"
            android:layout_below="@id/backarrow"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:textColor="@color/black"
            android:textSize="22dp"/>


        <TextView
            android:id="@+id/ATipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/img"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="39dp"
            android:text="Tipo"
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ATipo"
            android:layout_marginTop="30dp"
            android:text="Placas"
            android:textSize="18dp"
            android:textColor="@color/black"
            android:layout_marginLeft="20dp"
            android:id="@+id/APlacas"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/APlacas"
            android:layout_marginTop="30dp"
            android:text="Transmisión"
            android:textSize="18dp"
            android:textColor="@color/black"
            android:layout_marginLeft="20dp"
            android:id="@+id/ATransmision"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ATransmision"
            android:layout_marginTop="40dp"
            android:text="Precio por día"
            android:textSize="18dp"
            android:textColor="@color/black"
            android:layout_marginLeft="20dp"
            android:id="@+id/APrecio"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/APrecio"
            android:layout_marginTop="40dp"
            android:text="Total>"
            android:textSize="18dp"
            android:textColor="@color/black"
            android:layout_marginLeft="20dp"
            android:id="@+id/ATotal"/>


        <Button android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_below="@id/ATotal"
            android:background="@layout/boton_redondo"
            android:textColor="#FFF"
            android:text="Pagar"
            android:id="@+id/btnPagar"/>


        <TextView
            android:id="@+id/tipo1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/img"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="41dp"
            android:text="1"
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/tipo2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tipo1"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="29dp"
            android:text="1"
            android:textColor="@color/black"
            android:textSize="18dp" />



        <TextView
            android:id="@+id/tipo5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tipo2"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="38dp"
            android:text="1"
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/tipo6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tipo5"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="41dp"
            android:text="1"
            android:textColor="@color/black"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/tipo7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tipo6"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="41dp"
            android:text="1"
            android:textColor="@color/black"
            android:textSize="18dp" />


    </RelativeLayout>
</ScrollView>