<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Cliente.Home.AutoSelected">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modelo"
        android:id="@+id/AModelo"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:textColor="@color/black"
        android:textSize="18dp"/>

    <TextView
        android:id="@+id/AColor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="80dp"
        android:layout_toRightOf="@id/AModelo"
        android:text="Color"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/ATipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/AColor"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="39dp"
        android:text="Tipo"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ATipo"
        android:layout_marginTop="30dp"
        android:text="Placas"
        android:textSize="18dp"
        android:textColor="@color/black"
        android:layout_marginLeft="20dp"
        android:id="@+id/APlacas"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/APlacas"
        android:layout_marginTop="30dp"
        android:text="Foto"
        android:textSize="18dp"
        android:textColor="@color/black"
        android:layout_marginLeft="20dp"
        android:id="@+id/Afoto"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Afoto"
        android:layout_marginTop="30dp"
        android:text="Transmisión"
        android:textSize="18dp"
        android:textColor="@color/black"
        android:layout_marginLeft="20dp"
        android:id="@+id/ATransmision"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/ATransmision"
        android:layout_marginTop="40dp"
        android:text="Precio por día"
        android:textSize="18dp"
        android:textColor="@color/black"
        android:layout_marginLeft="20dp"
        android:id="@+id/APrecio"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/APrecio"
        android:layout_marginTop="40dp"
        android:text="Total>"
        android:textSize="18dp"
        android:textColor="@color/black"
        android:layout_marginLeft="20dp"
        android:id="@+id/ATotal"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modificar"
        android:layout_below="@id/ATotal"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="80dp"
        android:textColor="@color/white"
        android:id="@+id/btnModificar"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pagar"
        android:layout_below="@id/ATotal"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="80dp"
        android:textColor="@color/white"
        android:id="@+id/btnPagar"/>


    <TextView
        android:id="@+id/tipo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/AColor"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="41dp"
        android:text="1"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/tipo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tipo1"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="29dp"
        android:text="1"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/tipo3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tipo2"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="23dp"
        android:text="1"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/tipo5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tipo3"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="38dp"
        android:text="1"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/tipo6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tipo5"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="41dp"
        android:text="1"
        android:textColor="@color/black"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/tipo7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tipo6"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="41dp"
        android:text="1"
        android:textColor="@color/black"
        android:textSize="18dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Model"
        android:id="@+id/txtmodelo"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="45dp"
        android:textColor="@color/black"
        android:textSize="18dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Colorr"
        android:id="@+id/txtcolor"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="45dp"
        android:textColor="@color/black"
        android:textSize="18dp"/>
</RelativeLayout>