<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usuariocliente">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UsuarioCliente">
        <receiver android:name=".Models.MyLocation"/>
        <activity android:name=".Registro" />
        <activity android:name=".UsuarioDriverInfo.UsuarioDriverInfo" />
        <activity android:name=".UsuarioDriverHistorial.HistorialSelectedDriver" />
        <activity android:name=".UsuarioDriverHistorial.UsuarioDriverHistorial" />
        <activity android:name=".UsuarioDriverMenu.UsuarioDriverMenu" />
        <activity android:name=".UsuarioClienteConfiguracion.UsuarioClienteUserOptions" />
        <activity android:name=".UsuarioClienteHistorial.HistorialSelected" />
        <activity android:name=".UsuarioClienteHistorial.UsuarioClienteHistorial" />
        <activity android:name=".UsuarioClienteMenu.AutoList" />
        <activity android:name=".UsuarioClienteMenu.AutoSelected" />
        <activity android:name=".UsuarioClienteMenu.UsuarioClienteMenu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>